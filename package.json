{
  "name": "ey-techathon-rfp-automation",
  "version": "2.0.0",
  "description": "Agentic AI system for B2B RFP scanning and response automation - EY Techathon 6.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "precompute-embeddings": "node scripts/precompute-embeddings.js",
    "init-vector-store": "node scripts/init-vector-store.js",
    "init-learning": "node scripts/init-adaptive-learning.js",
    "init-learning:watch": "node scripts/init-adaptive-learning.js --watch",
    "finetune": "node -e \"import('./services/model-finetuner.js').then(m => m.runFineTuning())\"",
    "pipeline": "node pipeline.js",
    "pipeline:help": "node pipeline.js --help",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "generate-live-data": "node scripts/generate-live-dataset.js",
    "generate-hybrid-data": "node scripts/generate-hybrid-dataset.js",
    "update-portals": "node scripts/update-portal-files.js",
    "combine-data": "node scripts/combine-portal-data.js",
    "generate-pdfs": "node scripts/generate-professional-pdfs.js",
    "validate-config": "node -e \"import('./configs/settings.js').then(c => console.log(c.validateConfig()))\""
  },
  "dependencies": {
    "@aws-sdk/client-textract": "^3.0.0",
    "@google/genai": "^1.24.0",
    "@huggingface/transformers": "^3.0.0",
    "@langchain/core": ">=0.3.0",
    "@langchain/google-genai": ">=0.1.0",
    "@langchain/langgraph": ">=0.2.0",
    "axios": "^1.12.2",
    "better-sqlite3": "^12.4.1",
    "chart.js": "^4.5.1",
    "cheerio": "^1.1.2",
    "chromadb": "^3.1.8",
    "cookie-parser": "^1.4.7",
    "csv-parse": "^6.1.0",
    "dayjs": "^1.11.18",
    "docx": "^9.5.1",
    "docxtemplater": "^3.66.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.2",
    "fontkit": "^2.0.4",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "ics": "^3.8.1",
    "langchain": "^1.0.1",
    "mammoth": "^1.11.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.47.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "openai": "^6.9.1",
    "pdf-parse": "^2.4.3",
    "pdfkit": "^0.17.2",
    "pizzip": "^3.2.0",
    "puppeteer": "^24.25.0",
    "qrcode": "^1.5.4",
    "uuid": "^9.0.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "keywords": [
    "rfp",
    "tender",
    "ai",
    "agentic",
    "automation",
    "b2b",
    "procurement",
    "ey-techathon"
  ],
  "author": "EY Techathon 6.0 Team",
  "license": "MIT"
}
